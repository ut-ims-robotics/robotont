<?xml version="1.0" ?>
<launch>

  <include file="$(find robotont_driver)/launch/driver_teleop_joy.launch" />

  <param name="robot_description" textfile="$(find robotont_description)/urdf/robotont_description.urdf" />

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find robotont_description)/config/robotont_description.rviz" output="screen" />

</launch>
